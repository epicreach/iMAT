<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox prefHeight="139.0" prefWidth="920.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <AnchorPane maxHeight="190.0" maxWidth="185.0" minHeight="0.0" minWidth="0.0" prefHeight="140.0" prefWidth="180.0">
            <children>
                <ImageView fitHeight="93.0" fitWidth="89.0" layoutX="46.0" layoutY="23.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@{image}" />
                    </image>
                </ImageView>
                <Text layoutX="31.0" layoutY="126.0" strokeType="OUTSIDE" strokeWidth="0.0" text="{name}">
                    <font>
                        <Font name="System Bold" size="12.0" />
                    </font>
                </Text>
                <Text layoutX="31.0" layoutY="146.0" strokeType="OUTSIDE" strokeWidth="0.0" text="{price} kr">
                    <font>
                        <Font size="12.0" />
                    </font>
                </Text>
                <Button layoutX="116.0" layoutY="137.0" mnemonicParsing="false" onAction="#addToCart" prefHeight="25.0" prefWidth="51.0" text="Add" />
            </children>
        </AnchorPane>
    </children>
</HBox>
